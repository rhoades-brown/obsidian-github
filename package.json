		"@types/node": "^24.10.1",
		"@typescript-eslint/eslint-plugin": "^8.48.1",
		"@typescript-eslint/parser": "^8.48.1",
		"builtin-modules": "^5.0.0",
		"esbuild": "^0.27.1",
		"obsidian": "latest",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"octokit": "^5.0.5"
	}
}
