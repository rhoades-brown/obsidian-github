{
	"name": "github-octokit",
	"version": "0.1.0",
	"description": "Sync your Obsidian vault with GitHub using the Octokit API.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
